<?xml version="1.0" encoding="windows-1251" ?>
<ServiceProvider_Response>
    <ServiceInfo>
        <Amount Editable="N" AmountPrecision="%AMOUNT_PRECISION%">
            <Debt>%AMOUNT%</Debt>
            <Penalty></Penalty>
        </Amount>
        <Name>
            <FirstName>%FIRSTNAME%</FirstName>
            <Patronymic>%PATRONYMIC%</Patronymic>
            <Surname>%SURNAME%</Surname>
        </Name>
        <Address>
            <City>%CITY%</City>
            <Street>%STREET%</Street>
            <House>%HOUSE%</House>
            <Building>%BUILDING%</Building>
            <Apartament>%APARTAMENT%</Apartament>
        </Address>
        <Info xml:space="preserve">
      <InfoLine>Оплата заказа номер %ORDER_NUMBER%, </InfoLine>
      <InfoLine>Сумма %AMOUNT% бел. руб. </InfoLine>
      <InfoLine>%ORDER_INFO%</InfoLine>
    </Info>
    </ServiceInfo>
</ServiceProvider_Response>